{
  "compileOnSave": false,
  "compilerOptions": {
  "allowJs": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "typeRoots": [
    "./node_modules/@types"
  ],
    "lib": [
    "es2018",
    "dom"
  ],
    "paths": {
    "@shared/*": [
      "src/app/shared/*"
    ],
      "@state/*": [
      "src/app/+state/*"
    ],
      "@state": [
      "src/app/+state"
    ],
      "@api": [
      "src/data-access"
    ],
      "@env/*": [
      "src/environments/*"
    ]
  },
  "useDefineForClassFields": false
},
  "angularCompilerOptions": {
  "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
}
}

